{
  "version": "0.2.0",
  "configurations": [
    {
      "command": "npm run develop --watch-admin",
      "name": "Run npm develop",
      "request": "launch",
      "type": "node-terminal",
      "env": {
        "DATABASE_SSL": "false",
        "NODE_ENV": "development",
      },
      "skipFiles": [
        "<node_internals>/**"
      ],
      "cwd": "${workspaceFolder}",
    }
  ]
}